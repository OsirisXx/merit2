{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key",
    "FIREBASE_API_KEY": "@firebase-api-key",
    "FIREBASE_SERVER_KEY": "@firebase-server-key",
    "FIREBASE_PROJECT_ID": "@firebase-project-id"
  }
} 